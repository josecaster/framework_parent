<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-2.0.xsd">

	<changeSet onValidationFail="MARK_RAN" failOnError="true"
		author="simplesolutions" id="1585760612557">
		<preConditions onFail="MARK_RAN">
			<not>
				<tableExists tableName="CURRENCIES_" />
			</not>
		</preConditions>
		<createTable tableName="CURRENCIES_">
			<column name="ID_" type="BIGINT">
				<constraints nullable="false" />
			</column>
			<column name="ACTIVE_" type="BIT(1)" />
			<column name="NAME_" type="VARCHAR(200)" />
			<column name="CODE_" type="VARCHAR(100)" />

		</createTable>
	</changeSet>
	<changeSet onValidationFail="MARK_RAN" failOnError="true"
		author="simplesolutions" id="1585760614563">
		<preConditions onFail="MARK_RAN">
			<not>
				<tableExists tableName="CURRENCIES_AUD" />
			</not>
		</preConditions>
		<createTable tableName="CURRENCIES_AUD">
			<column name="ID_" type="BIGINT">
				<constraints nullable="false" />
			</column>
			<column name="REV" type="BIGINT">
				<constraints nullable="false" />
			</column>
			<column name="REVISION_TYPE" type="TINYINT" />
			<column name="ACTIVE_" type="BIT(1)" />
			<column name="NAME_" type="VARCHAR(200)" />
			<column name="CODE_" type="VARCHAR(100)" />

		</createTable>
	</changeSet>

</databaseChangeLog>